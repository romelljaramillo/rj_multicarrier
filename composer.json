{
  "name": "roanja/rj_multicarrier",
  "description": "Roanja Multi-carrier shipping module modernized for PrestaShop 8",
  "type": "prestashop-module",
  "license": "AFL-3.0",
  "require": {
    "php": ">=8.1",
    "ramsey/uuid": "^4.3",
    "iio/libmergepdf": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5 || ^8.5 || ^9.5"
  },
  "autoload": {
    "psr-4": {
      "Roanja\\Module\\RjMulticarrier\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Roanja\\Module\\RjMulticarrier\\Tests\\": "tests/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "optimize-autoloader": true
  }
}
